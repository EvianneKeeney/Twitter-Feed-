import React from 'react';
import Question from './Question'

class App extends React.Component{
  constructor(props){
    super(props);
  }


  render(){
    let questions = this.props.data.map((question) =>

    return(
      <div>
        < Question
          key = {question.id}
          text = {question.body}
          />
      </div>
    )

    return(
      <div>
        <h1>React Quiz!</h1>
        {questions}
      </div>
      )
  )}
}

export default App;
